type Chat {
    id: ID!
    message: String!
    sender: User!
    receiver: User!
    timestamp: String!
}
type User {
    id: ID!
    firstName: String!
    lastName: String!
    email: String!
}

type Mutation {
    sendMessage(receiver: String!, message: String!): Chat!
}
type Query {
    getChatByUser: [Chat]
    getChatForAdmin(userEmail: String!): [Chat]
}

